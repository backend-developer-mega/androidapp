<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">
    <item>
        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="#e3e3e3"/>
            <solid android:color="#ffdddddd"/>
        </shape>
    </item>
    <item android:top="0dp" android:bottom="2dp">
        <shape android:shape="rectangle">
            <stroke android:width="1dp" android:color="#e3e3e3"/>
            <solid android:color="@color/colorWhite"/>
        </shape>
    </item>
</layer-list>
